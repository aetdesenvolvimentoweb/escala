generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  userName String @unique
  password String

  @@map("users")
}

model Military {
  id         String @id @default(auto()) @map("_id") @db.ObjectId
  graduation String
  rg         Int    @unique
  name       String

  @@map("military")
}

model Vehicles {
  id   String @id @default(auto()) @map("_id") @db.ObjectId
  name String @unique

  @@map("vehicles")
}
